{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "eca",
    "npmScope": "eca",
    "latestMigration": "20180424-add-tsconfig-tools"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/lib-demo-eca-client/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/lib-demo-eca-client/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "apps": [
    {
      "name": "lib-demo-eca-client",
      "root": "apps/lib-demo-eca-client/src",
      "outDir": "dist/apps/lib-demo-eca-client",
      "assets": [
        "assets",
        "favicon.ico",
        "config.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "eca",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "tags": []
    },
    {
      "name": "client-lib",
      "root": "libs/eca-client/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tsconfig": "tsconfig.packages.json",
      "tags": []
    }
  ],
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    },
    "lintFix": true,
    "styleExt": "css",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
