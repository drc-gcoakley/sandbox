<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="PRESELECT_RENAME" value="false" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bTODO GLEN\b">
      <option name="FOREGROUND" value="c000c0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>