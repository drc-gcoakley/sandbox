<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="3" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="PRESELECT_RENAME" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bTODO GLEN\b">
      <option name="FOREGROUND" value="3c00c8" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="3c00c8" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bGAC\b">
      <option name="FOREGROUND" value="3c00c8" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="3c00c8" />
    </pattern>
    <pattern case-sensitive="true" pattern="\bTODO\b" />
  </component>
</application>