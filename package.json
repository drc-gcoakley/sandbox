{
  "name": "client-lib",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "main": "dist/@eca/client-lib/index.js",
  "scripts": {
    "NOTE_1a": "Avoid the temptation to have these scripts call other scripts. Each one causes ",
    "NOTE_1b": "another copy of node to be invoked and will prevent proper handling of signals ",
    "NOTE_1c": "such as ctrl-c which means the processes will not be killed.",
    "start": "ng serve -v --port 4321",
    "build:all": "yarn build:libs && yarn build:app",
    "build:app": "ng build --app lib-demo-eca-client --prod",
    "build:libs": "yarn build:eca-client-lib && yarn copy-assets:eca-client-lib",
    "build:eca-client-lib": "ng-packagr -p libs/eca-client/package.json",
    "copy-assets:eca-client-lib": "copyfiles -u 3 libs/eca-client/src/config.json dist/@eca/client-lib",
    "clean": "rimraf dist/*",
    "clean-start": "yarn clean && yarn build:all && yarn verify && yarn start",
    "test": "ng test --watch=false",
    "lint": "ng lint --format verbose && ./node_modules/.bin/nx lint",
    "lint-fix": "ng lint --fix --format verbose",
    "coverage": "ng test --code-coverage --watch=false",
    "verify": "yarn test && yarn e2e && yarn lint",
    "e2e": "ng e2e --app lib-demo-eca-client",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0t",
    "@ngrx/store-devtools": "^5.2.0",
    "@types/angular": "1.6.43",
    "@types/node": "~6.0.60",
    "@types/react": "^16.3.12",
    "core-js": "^2.4.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.5",
    "ngrx-store-freeze": "^0.2.2",
    "rxjs": "^5.5.10",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.3.2",
    "@angular-devkit/schematics": "^0.3.2",
    "@angular/cli": "^1.7.4",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@ngrx/schematics": "^5.2.0",
    "@nrwl/nx": "^1.0.3",
    "@nrwl/schematics": "^1.0.3",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/karma": "^1.7.3",
    "codelyzer": "^4.0.1",
    "copyfiles": "^2.0.0",
    "cosmiconfig": "^2.2.2",
    "fs-extra": "^5.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-typescript": "^3.0.12",
    "path": "^0.12.7",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-angular": "^1.1.1",
    "typescript": "2.6.2"
  }
}
